	<variable name="foo" negated="false"/>
	<disjunction>
		<variable name="bar" negated="true"/>
		<variable name="baz" negated="false"/>
	</disjunction>
	<conjunction>
		<variable name="bar" negated="true"/>
		<conjunction>
			<variable name="foo" negated="false"/>
